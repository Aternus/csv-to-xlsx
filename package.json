{
  "name": "@aternus/csv-to-xlsx",
  "version": "1.0.0",
  "description": "Node.js command-line tool and a library to convert CSV files to XLSX (Excel 2007+ XML Format) files.",
  "keywords": [
    "convert",
    "csv",
    "xlsx",
    "excel",
    "xml",
    "utf-8",
    "multilingual",
    "batch"
  ],
  "main": "dist/csv-to-xlsx.js",
  "bin": {
    "csv-to-xlsx": "dist/csv-to-xlsx.js",
    "csv-to-xlsx-win": "bin/csv-to-xlsx-win.exe",
    "csv-to-xlsx-linux": "bin/csv-to-xlsx-linux",
    "csv-to-xlsx-macos": "bin/csv-to-xlsx-macos"
  },
  "scripts": {
    "build:src": "npx babel -d ./dist ./src",
    "build:dist": "npx babel -d ./dist ./src --minified --no-comments",
    "build:bin": "npx pkg --out-path ./bin .",
    "test:raw": "npx mocha",
    "test": "npm-run-all build:src test:raw",
    "start:raw": "node ./dist/csv-to-xlsx.js",
    "start": "npm-run-all build:src start:raw",
    "prepublishOnly": "npm-run-all build:** test"
  },
  "author": "Kiril Reznik",
  "license": "MIT",
  "homepage": "https://github.com/Aternus/csv-to-xlsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aternus/csv-to-xlsx.git"
  },
  "bugs": {
    "url": "https://github.com/Aternus/csv-to-xlsx/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@types/chai": "^4.1.4",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^8.10.29",
    "chai": "^4.1.2",
    "eslint": "^5.6.0",
    "eslint-config-google": "^0.10.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "pkg": "^4.3.4"
  },
  "dependencies": {
    "commander": "^2.18.0",
    "csv-parse": "^3.0.0",
    "fs-extra": "^7.0.0",
    "xlsx": "^0.14.0"
  }
}
